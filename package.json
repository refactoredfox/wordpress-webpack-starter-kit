{
  "name": "wordpress-webpack-starter-kit",
  "version": "0.4.0",
  "description": "WordPress, Docker, Babel and Mocha build with webpack",
  "scripts": {
    "start-message": "babel-node src/tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start:test": "npm-run-all --parallel test:watch open:src lint:watch",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node src/tools/_start.js",
    "build": "babel-node src/tools/_build.js",
    "lint": "esw webpack.config.* src/js/module",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha src/tools/testSetup.js \"./**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./src/tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Fifteen Four",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "name": "0.0.2",
    "object-assign": "4.1.1",
    "random-seed": "0.3.0"
  },
  "devDependencies": {
    "autoprefixer": "7.2.4",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.2",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "6.26.0",
    "browser-sync": "2.23.2",
    "bs-html-injector": "3.0.3",
    "chai": "4.1.2",
    "chalk": "^2.1.0",
    "cheerio": "1.0.0-rc.2",
    "connect-history-api-fallback": "1.5.0",
    "coveralls": "3.0.0",
    "cross-env": "5.1.3",
    "css-loader": "0.28.7",
    "eslint": "4.14.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "eslint-watch": "3.1.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "1.1.6",
    "fs-extra": "5.0.0",
    "glob": "7.1.2",
